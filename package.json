{
  "name": "rtek-footer",
  "version": "1.1.0",
  "description": "A reusable footer web component for r-TEK client websites",
  "main": "dist/rtek-footer.min.js",
  "module": "src/rtek-footer.js",
  "unpkg": "dist/rtek-footer.min.js",
  "jsdelivr": "dist/rtek-footer.min.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/rtek-footer.js --bundle --minify --outfile=dist/rtek-footer.min.js",
    "dev": "esbuild src/rtek-footer.js --bundle --outfile=dist/rtek-footer.js --watch --servedir=."
  },
  "keywords": [
    "web-component",
    "footer",
    "rtek",
    "custom-element"
  ],
  "author": "r-TEK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtek/footer"
  },
  "devDependencies": {
    "esbuild": "^0.20.0"
  }
}
